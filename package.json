    "fuzzysort": "^3.1.0",
    "glob": "^13.0.0",
    "lucide-react": "^0.563.0",
    "minimatch": "^10.1.1",
    "next": "16.1.5",
    "next-themes": "^0.4.6",
    "ora": "^9.1.0",
    "p-map": "^7.0.4",
    "p-retry": "^7.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "recharts": "^3.7.0",
    "remark-gfm": "^4.0.1",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "unist-util-visit": "^5.1.0",
