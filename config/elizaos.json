  "PIPELINE_SCORING": {
    "pullRequest": {
      "base": 4,
      "merged": 16,
      "perReview": 1.5,
      "perApproval": 2,
      "perComment": 0.2,
      "descriptionMultiplier": 0.003,
      "complexityMultiplier": 0.5,
      "optimalSizeBonus": 5,
      "maxPerDay": 10,
      "closingIssueBonus": 5
    },
    "reaction": {
      "diminishingReturns": 0.7,
      "base": 0.5,
      "received": 0.1,
      "maxPerDay": 10,
      "types": {
        "thumbs_up": 1.2,
        "thumbs_down": 0.5,
        "laugh": 1.0,
        "hooray": 1.5,
        "confused": 0.5,
        "heart": 1.5,
        "rocket": 1.5,
        "eyes": 1.2
      }
    },
    "issue": {
      "base": 2,
      "perComment": 0.1,
      "withLabelsMultiplier": {
        "bug": 1.8,
        "enhancement": 1.4,
        "documentation": 1.0
      },
      "closedBonus": 2,
      "resolutionSpeedMultiplier": 1.0
    },
    "review": {
      "base": 4,
      "approved": 1,
      "changesRequested": 2,
      "commented": 0.5,
      "detailedFeedbackMultiplier": 0.002,
      "thoroughnessMultiplier": 1.3,
      "maxPerDay": 8
    },
    "comment": {
      "base": 0.2,
      "substantiveMultiplier": 0.001,
      "diminishingReturns": 0.7,
      "maxPerThread": 3
    },
    "codeChange": {
      "perLineAddition": 0.005,
      "perLineDeletion": 0.01,
      "perFile": 0.15,
      "maxLines": 800,
      "testCoverageBonus": 2.0
    }
  },

