                  "$ref": "#/components/schemas/SummaryIndexResponse"
                },
                "example": {
                  "version": "1.0",
                  "type": "overall",
                  "interval": "week",
                  "generatedAt": "2026-01-15T00:53:30.917Z",
                  "items": [
                    {
                      "date": "2026-01-12",
                      "path": "/api/summaries/overall/week/2026-01-12.json",
                      "contentHash": "3b5f5b9494a3098171038bb9156a8a2fb19dd9ec"
                    },
                    {
                      "date": "2026-01-05",
                      "path": "/api/summaries/overall/week/2026-01-05.json",
